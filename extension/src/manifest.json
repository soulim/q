{
  "manifest_version": 2,
  "name": "Q",
  "version": "0.1.0",
  "permissions": [
    "nativeMessaging"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "q@sulim.dev",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "q-icon-16.png",
      "32": "q-icon-32.png",
      "64": "q-icon-64.png"
    },
    "default_title": "Q",
    "default_popup": "popup.html",
    "browser_style": true
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Q"
      }
    }
  },
  "icons": {
    "16": "q-icon-16.png",
    "32": "q-icon-32.png",
    "64": "q-icon-64.png"
  }
}
